
<%= form_with model: @user, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %>
  <%= form.label :email %>
  <%= form.text_field :email %>
  <%= form.label :password %>
  <%= form.text_field :pasword %>
  <%= form.label :confirm_password %>
  <%= form.text_field :confirm_password %>
  <%= form.submit "Register" %>
<% end %>
